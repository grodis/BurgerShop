<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>BeefTechee - Dashboard</title>

    <!-- Custom fonts for this template-->
    <link href="~/Content/admin-panel/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

    <!-- Custom styles for this template-->
    <link href="~/Content/admin-panel/css/sb-admin-2.min.css" rel="stylesheet">

    <link href="~/Content/admin-panel/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">
    <link href="~/Content/admin-panel/css/myAdminCss.css" rel="stylesheet" />

</head>

<body id="page-top">

    <!-- loading -->
    <!-- Page Wrapper -->
    <div id="wrapper">

        @Html.Partial("_AdminSidebar")
        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">
                @Html.Partial("_AdminTopbar")
                @RenderBody()
                @Html.Partial("_AdminFooter")

            </div>
            <!-- End of Content Wrapper -->

        </div>
        <!-- End of Page Wrapper -->
    </div>
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>



    <!-- Bootstrap core JavaScript-->
    <script src="~/Content/admin-panel/vendor/jquery/jquery.min.js"></script>
    <script src="~/Scripts/bootstrap.bundle.min.js"></script>
    <!-- Core plugin JavaScript-->
    <script src="~/Scripts/jquery.easing.min.js"></script>
    <!-- Custom scripts for all pages-->
    <script src="~/Content/admin-panel/js/sb-admin-2.min.js"></script>


    <!-- Page level plugins -->
    <script src="~/Content/admin-panel/vendor/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Content/admin-panel/vendor/datatables/dataTables.bootstrap4.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="~/Content/admin-panel/js/demo/datatables-demo.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
    <script src="~/Scripts/globalize.js"></script>
    <script src="~/Scripts/globalize/currency.js"></script>
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/jQueryFixes.js"></script>
    <script src="~/Scripts/GoogleMapsAdmin.js"></script>

</body>

</html>
